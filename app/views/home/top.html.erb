<div class="main-search">
    <div class="container">
        <div class="main-search-wrapper">
            <h2>Room Shareで<br>
            まだ体験したことのない旅をしよう</h2>
            <div class="search-wrapper">
             <%= form_with(url: rooms_search_path) do |f| %>
              <%= f.label :search, "エリア" %>
              <%= f.search_field :search, class: "main-search-field", placeholder:"東京" %>
              <%= f.submit "探す", class:"btn search"%>
             <% end %>
            </div>
        </div>
    </div>
</div>
<div class="area">
    <div class="container">
        <h2>おすすめのエリア</h2>
        <div class="area-wrapper">
            <div class="area-content">
                <%= link_to(rooms_tokyo_path) do %>
                 <div class="content-image">
                    <img class="area-img" src="https://www.knt.co.jp/tabiplanet/image/kokunai_200221_mv.jpg">
                    <p>東京</p>
                 </div>
                <% end %>
            </div>
            <div class="area-content">
                <%= link_to(rooms_osaka_path) do %>
                 <div class="content-image">
                    <img class="area-img" src="https://rimage.gnst.jp/livejapan.com/public/article/detail/a/20/00/a2000096/img/basic/a2000096_main.jpg?20201224090727&q=80&rw=750&rh=536">
                 <p>大阪</p>
                 </div>
                <% end %>
            </div>
            <div class="area-content">
                <%= link_to(rooms_kyoto_path) do %>
                 <div class="content-image">
                    <img class="area-img" src="data:image/jpeg;base64,">
                    <p>京都</p>
                 </div>
                <% end %>
            </div>
            <div class="area-content">
                <%= link_to(rooms_sapporo_path) do %>
                 <div class="content-image">
                    <img class="area-img" src="https://www.tabirai.net/sightseeing/column/img/0001391/kiji1Img.JPG?uid=20160309220007">
                    <p>札幌</p>
                 </div>
                <% end %>
            </div>
        </div>
    </div>
</div>